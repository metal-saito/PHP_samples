{
    "name": "metal-saito/php-samples",
    "description": "PHP & Laravel technical samples for Findy portfolio",
    "type": "project",
    "keywords": ["php", "laravel", "samples", "portfolio"],
    "license": "MIT",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "samples/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

